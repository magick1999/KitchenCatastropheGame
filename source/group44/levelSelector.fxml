<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?group44-stylesheet application.css?>

<BorderPane id="root" fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="group44.LevelSelectorController">
   <top>
      <ImageView id="logo" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
         <image>
            <Image url="@kitchenLogo.png" />
         </image></ImageView>
   </top>
   <bottom>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="100" BorderPane.alignment="CENTER">
         <children>
            <Button id="button" fx:id="previous" alignment="CENTER" mnemonicParsing="false" text="Previous" />
            <Button id="button" fx:id="play" mnemonicParsing="false" text="Button" />
            <Button id="button" fx:id="next" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="Next" />
            <Button id="button" fx:id="menu" mnemonicParsing="false" text="Main Menu" />
         </children>
      </HBox>
   </bottom>
   <center>
      <VBox id="levelInfo" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button id="button" fx:id="levelNum" mnemonicParsing="false" text="Level 1" />
            <ListView fx:id="levelTimes" prefHeight="200.0" prefWidth="200.0" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
