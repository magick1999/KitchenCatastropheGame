<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="group44.LevelSelectorController">
   <center>
      <GridPane fx:id="levels" alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button id="button" fx:id="level0" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" stylesheets="@resources/application.css" text="Level 1" GridPane.columnIndex="0" GridPane.rowIndex="0" />
            <Button id="button" fx:id="level1" alignment="CENTER" mnemonicParsing="false" stylesheets="@resources/application.css" text="Level 2" GridPane.columnIndex="1" GridPane.rowIndex="0" />
            <Button id="button" fx:id="level2" alignment="CENTER" mnemonicParsing="false" stylesheets="@resources/application.css" text="Level 3" GridPane.columnIndex="0" GridPane.rowIndex="1" />
            <Button id="button" fx:id="level3" alignment="CENTER" mnemonicParsing="false" stylesheets="@resources/application.css" text="Level 4" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </GridPane>
   </center>
   <top>
      <Button id="button" fx:id="logo" mnemonicParsing="false" stylesheets="@resources/application.css" text="Button" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="100" BorderPane.alignment="CENTER">
         <children>
            <Button id="button" fx:id="previous" alignment="CENTER" mnemonicParsing="false" stylesheets="@resources/application.css" text="Previous" />
            <Button id="button" fx:id="next" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" stylesheets="@resources/application.css" text="Next" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
